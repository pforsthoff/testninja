// Scripted Pipeline with stages
node ('worker_node1') {
  stage('Source') { // Get code
    git( 
                        url: 'https://github.com/pforsthoff/testninja.git/', 
                        branch: 'master'
                        )
  }
  stage('Build (dotnet build)') {
            steps {
                sh 'echo "Building dotnet app"'
                sh 'dotnet build'
            }
        }
}